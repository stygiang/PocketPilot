{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "eslint app components lib",
    "typecheck": "tsc -p tsconfig.json",
    "postinstall": "pnpm -C ../../packages/db prisma:generate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:report": "playwright show-report",
    "test:e2e:update-snapshots": "playwright test --update-snapshots"
  },
  "dependencies": {
    "@safetospend/core": "workspace:*",
    "@safetospend/db": "workspace:*",
    "@safetospend/validators": "workspace:*",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.1.3",
    "framer-motion": "^12.26.2",
    "jose": "^5.6.3",
    "next": "^14.2.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^4.0.0",
    "sass": "^1.97.2",
    "stripe": "^16.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.4"
  }
}
